{
    "name": "thesgroup/module-webauthn",
    "description": "The Magento 2 module which provides ability to login using the WebAuthn",
    "homepage": "https://www.sashas.org/",
    "type": "magento2-module",
    "require": {
        "php": "~7.3.0||~7.4.0",
        "magento/framework": "~102.0||~103.0",
        "magento/module-customer": "~103.0",
        "web-auth/webauthn-lib": "~3.2.||~3.3",
        "nyholm/psr7-server": ">=1.0.0",
        "nyholm/psr7": ">=1.3.1"
    },
    "require-dev": {
        "thesgroup/magento2-testing-framework": "~1.0.4"
    },
    "license": [
        "GPL-3.0-only"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "autoload": {
        "files": [ "registration.php" ],
        "psr-4": {
            "TheSGroup\\WebAuthn\\": ""
        }
    }
}
